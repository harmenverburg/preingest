@model List<Noord.HollandsArchief.Pre.Ingest.WebTool.Entities.ProcessResult>
<div class="card m">
    <div class="card-body">
        <h5 class="card-title">Virusscan resultaten</h5>
        @{ 
            if (Model == null)
            {
                <p class="card-text">Virusscan niet gestart.</p>
            }

            else if (Model.Count == 0)
            {
                <p class="card-text">Geen bijzonderheden gevonden.</p>
            }
            else
            {
                @foreach (Noord.HollandsArchief.Pre.Ingest.WebTool.Entities.ProcessResult item in Model)
                {<p class="card-text">@item.Message</p>}
            }
        }
    </div>
</div>
