@{
<div class="card m">        
    <div class="card-body">
        <h5 class="card-title">Topx</h5>
        @if (!String.IsNullOrEmpty(Model))
        {
            <p class="card-text">
                @Html.Raw(@Model.ToString())
            </p>
        }
        else
        { <p class="card-text">Topx data niet gevonden.</p>}
    </div>
</div>
}
